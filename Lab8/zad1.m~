close all; clearvars; clc;

dom = imread('dom.png');

edge_dom = edge(dom, 'log');
zero_dom = edge(dom, 'zerocross');
canny_dom = edge(dom, 'canny');
sobel_dom = edge(dom, 'Sobel');

edge_dom_man = edge(dom, 'log', 0.5, 0.5);


figure();
subplot(5,2,1);
imshow(dom);
title('original');
subplot(5,2,2);
imshow(zero_dom);
title('log');
subplot(4,1,3);
imshow(edge_dom);
title('zero-crossing');

T1 = 1;
T2 = 2;
sigma = 1;

subplot(4,1,4);
imshow(canny_dom);
title('canny');

